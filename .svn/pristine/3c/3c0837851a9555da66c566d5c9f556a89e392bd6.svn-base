html {background:url("../img/hd_bg.png") repeat-x 0 0 #f3f3f3}
body {font-family:Microsoft Yahei; font-size:12px; color:#333; overflow-y:scroll}
html.dialog {background:white;}

body .YSL_MAKSER {background-color:gray;}
body .PopupDialog {box-shadow:0 0 8px gray;}

a {text-decoration:none; color:#567;}
a:hover {text-decoration:underline}
.header, .container, .footer {width:1000px; margin:0 auto;}
.header {height:70px; clear:both; overflow:hidden; position:relative;}
.header a {color:white; font-size:13px;}
.container-wrap {background-color:white; border-top:1px solid #fff; border-bottom:1px solid #fff; padding-bottom:20px; box-shadow:0px 0px 2px gray;}
.logo {font-size:32px; float:left; margin:15px 0 0; height:43px; width:136px; background:url("../img/logo.png") no-repeat 0 0;}
.logo a {display:block; height:43px; text-indent:-9999em;}
.logo a:hover {text-decoration:none}
.site-desc {margin:30px 0 0 8px; float:left; font-size:13px; color:white; text-shadow:1px 1px 1px black;}
.site-shortcut {position:absolute; bottom:5px; right:5px; color:white;}
.footer {clear:both; padding:15px 0; color:gray;}

.site-nav {position:absolute; top:0px; right:0; padding:5px 15px 8px 10px}
.site-nav li {float:left; margin-left:1em}

.site-links {overflow:hidden; margin-bottom:5px}
.site-links li {float:left; margin-right:10px}

.page-tip {background-color:#eee; line-height:1.75em; padding:10px; border-radius:10px; font-size:13px; color:gray;}

.left-col {float:left; width:700px; padding:0 10px; margin-top:10px;}
.right-col {float:right; width:260px; padding-right:10px; margin-top:10px; }

input.txt, textarea.txt {border:1px solid #eee; border-top-color:#ddd; border-left-color:#ddd; font-size:14px; padding:2px 2px; border-radius:3px}
input.date-txt {width:120px !important;}

/**
input.btn, a.btn, span.btn {border:1px solid #ccc; border-top-color:#ddd; border-left-color:#ddd; font-size:14px; padding:2px 10px; border-radius:3px; cursor:pointer; background-color:#fff; box-shadow:1px 1px 1px #ddd; text-shadow:1px 1px 1px #eee;}
input.b-btn, a.b-btn, span.b-btn {padding:8px 15px}
**/
.btn {height:28px; line-height:27px; text-align:center; font-size:14px; cursor:pointer; padding:0 10px;}
.btn span {position:relative; top:-1px;}
.btn-disable:hover {cursor:default;text-decoration:none }

.btn-strong {
	min-width:28px;
	box-shadow:0 1px 0 0 #d9d9d9;
	background-color:#5caae6;
	border-radius:2px;
	border:1px solid  #4899d7;
	display:inline-block;
	color:#fff;
	text-decoration:none}

.btn-strong:hover {
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #56a1da),color-stop(1, #67b2ec));
	background:-moz-linear-gradient(top, #56a1da 5%, #67b2ec 100 % );
	background:-webkit-linear-gradient(top, #56a1da 5%, #67b2ec 100 % );
	background:-o-linear-gradient(top, #56a1da 5%, #67b2ec 100 % );
	background:-ms-linear-gradient(top, #56a1da 5%, #67b2ec 100 % );
	background:linear-gradient(to bottom, #56a1da 5%, #67b2ec 100 % );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr = '#56a1da', endColorstr = '#67b2ec', GradientType = 0);
	background-color:#67b2ec;text-decoration:none }

.btn-strong:active {
	background:-webkit-gradient(linear,left top,left bottom,color-stop(0.05, #56a1da),
	color-stop(1, #549ad1));
	background:-moz-linear-gradient(top, #56a1da 5 % , #549bd1 100%);
	background:-webkit-linear-gradient(top, #56a1da 5 % , #549bd1 100%);
	background:-o-linear-gradient(top, #56a1da 5 % , #549bd1 100%);
	background:-ms-linear-gradient(top, #56a1da 5 % , #549bd1 100%);
	background:linear-gradient(to bottom, #56a1da 5 % , #549bd1 100%);
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr= '#56a1da', endColorstr = '#549bd1', GradientType = 0);
	background-color:#56a1da;box-shadow:inset 0 3px 3px rgba(65,122,165,.21) }

.btn-weak {
	-moz-box-shadow:0 1px 0 0  #d9d9d9;
	-webkit-box-shadow:0 1px 0 0 #d9d9d9;
	box-shadow:0 1px 0 0 #d9d9d9;
	-moz-border-radius:2px;
	-webkit-border-radius:2px;
	border-radius:2px;
	border:1px solid #dfdfdf;
	display:inline-block;
	color:#5f5f5f;
	text-decoration:none;
	background:-webkit-gradient(linear,left top,left bottom,color-stop(0.05, #FBFBFB),
	color-stop(1, #F2F2F2));
	background:-moz-linear-gradient(top, #FBFBFB 5 % , #F2F2F2 100 % );
	background:-webkit-linear-gradient(top, #FBFBFB 5 % , #F2F2F2 100 % );
	background:-o-linear-gradient(top, #FBFBFB 5 % , #F2F2F2 100 % );
	background:-ms-linear-gradient(top, #FBFBFB 5 % , #F2F2F2 100 % );
	background:linear-gradient(to bottom, #FBFBFB 5 % , #F2F2F2 100 % );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr = '#FBFBFB', endColorstr = '#F2F2F2', GradientType = 0);
	background-color:#F2F2F2;
	text-decoration:none }

.btn-weak:hover {
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #FAFAFA), color-stop(1, #EEE));
	background:-moz-linear-gradient(top, #FAFAFA 5 % , #EEE 100 % );
	background:-webkit-linear-gradient(top, #FAFAFA 5 % , #EEE 100 % );
	background:-o-linear-gradient(top, #FAFAFA 5 % , #EEE 100 % );
	background:-ms-linear-gradient(top, #FAFAFA 5 % , #EEE 100 % );
	background:linear-gradient(to bottom, #FAFAFA 5 % , #EEE 100 % );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr = '#FAFAFA', endColorstr = '#EEEEEE', GradientType = 0);
	background-color:#EEE;
	text-decoration:none }

.btn-weak:active {
	background:-webkit-gradient(linear, left top, left bottom, color-stop(0.05, #F0F0F0), color-stop(1, #F4F4F4));
	background:-moz-linear-gradient(top, #F0F0F0 5 % , #F4F4F4 100 % );
	background:-webkit-linear-gradient(top, #F0F0F0 5 % , #F4F4F4 100 % );
	background:-o-linear-gradient(top, #F0F0F0 5 % , #F4F4F4 100 % );
	background:-ms-linear-gradient(top, #F0F0F0 5 % , #F4F4F4 100 % );
	background:linear-gradient(to bottom, #F0F0F0 5 % , #F4F4F4 100 % );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr = '#56a1da', endColorstr = '#549bd1', GradientType = 0);
	background-color:#F4F4F4;
	box-shadow:inset 0 3px 2px rgba(176, 176, 176, .07);
	color:#5F5F5F }

.btn-disable {
	-moz-box-shadow:0 1px 0 0 #d9d9d9;
	-webkit-box-shadow:0 1px 0 0 #d9d9d9;
	box-shadow:0 1px 0 0 #d9d9d9;
	-moz-border-radius:2px;
	-webkit-border-radius:2px;
	border-radius:2px;
	border:1px solid #dfdfdf;
	display:inline-block;
	color:#5f5f5f;
	text-decoration:none;
	background:-webkit-gradient(linear,left top,left bottom,color-stop(0.05, #F0F0F0),
	color-stop(1, #F4F4F4));
	background:-moz-linear-gradient(top, #F0F0F0 5 % , #F4F4F4 100 % );
	background:-webkit-linear-gradient(top, #F0F0F0 5 % , #F4F4F4 100 % );
	background:-o-linear-gradient(top, #F0F0F0 5 % , #F4F4F4 100 % );
	background:-ms-linear-gradient(top, #F0F0F0 5 % , #F4F4F4 100 % );
	background:linear-gradient(to bottom, #F0F0F0 5 % , #F4F4F4 100 % );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr = '#F0F0F0', endColorstr = '#F4F4F4', GradientType = 0);
	background-color:#F4F4F4;
	box-shadow:inset 0 3px 2px rgba(176, 176, 176, .07);
	color:#C3C3C3 }
	
.switch-btn {display:inline-block; height:28px; line-height:28px; border:1px solid #aaa; border-radius:5px; font-size:12px; font-weight:normal; background-color:#ccc; padding:1px 20px 1px 1px}
.switch-btn s {display:inline-block; height:26px; line-height:25px; border:1px solid #ccc; border-radius:5px; box-shadow:1px 1px 1px gray; padding:0 8px;  text-decoration:none; background-color:white;}

.frm {margin-bottom:10px;}
.frm fieldset {padding:10px; border:none}
.frm legend {font-size:16px; font-weight:bold; margin:5px 0 -5px 0 !important; margin:0 0 0 -4px;}
.frm dl {padding:3px;}
.frm dt {float:left; padding:7px 0 0; width:90px; text-align:right;}
.frm dd {padding:3px; margin-left:100px}
.frm span.tip {color:#aaa; margin-left:0.5em}
.frm span.warn {color:red; margin-left:0.5em; padding-left:18px; background:url("../img/warn.gif") no-repeat left center; }
.frm span.required_star {color:red; font-weight:bold; font-size:18px; padding:0 5px; vertical-align:middle;}
.frm .field-desc {color:#aaa; margin-top:5px}
.frm .radio {vertical-align:middle}
.frm dd label {margin-right:1em;}
form.readonly input.txt {cursor:default; border:none;}

.tbl {border-collapse:collapse; border-spacing:0; border:1px solid #ccc; font-size:13px; margin-bottom:20px}
.tbl caption {text-align:left; padding:3px;}
.tbl thead tr {border-bottom:1px solid #ddd; border-top:1px solid #ccc}
.tbl th {color:#555; background-color:#eee; padding:3px 7px; text-shadow:1px 1px 1px #eee; text-align:left; text-transform:capitalize;}
.tbl tbody tr {border-bottom:1px dotted #ccc}
.tbl td {padding:10px 7px;}
